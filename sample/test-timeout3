#!/usr/bin/env ruby

require 'chkbuild'

ChkBuild.def_target("timeout3", :timeout=>'10s', :output_interval_timeout=>'3s') {|b|
  b.run("sh", "-c", 'sleep 60; echo 123456789 123456789  123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789')
}

ChkBuild.main
